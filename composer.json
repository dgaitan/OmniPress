{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "8.1",
        "automattic/woocommerce": "^3.0",
        "cknow/laravel-money": "^7.0",
        "filament/filament": "^2.0",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "http-interop/http-factory-guzzle": "^1.2",
        "inertiajs/inertia-laravel": "^0.5.2",
        "joelbutcher/socialstream": "^3.3",
        "laravel/cashier": "^13.8",
        "laravel/framework": "^9.0",
        "laravel/jetstream": "^2.5",
        "laravel/sanctum": "^2.8",
        "laravel/scout": "^9.4",
        "laravel/slack-notification-channel": "^2.4",
        "laravel/tinker": "^2.5",
        "lorisleiva/laravel-actions": "^2.4",
        "maatwebsite/excel": "^3.1",
        "meilisearch/meilisearch-php": "0.24.2",
        "myclabs/php-enum": "^1.8",
        "opcodesio/log-viewer": "1.4.4",
        "predis/predis": "^1.1",
        "spatie/laravel-data": "^1.2",
        "spatie/laravel-permission": "^5.4",
        "tightenco/ziggy": "^1.0",
        "web64/laravel-cli-colors": "^0.20.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "barryvdh/laravel-ide-helper": "^2.10",
        "fakerphp/faker": "^1.9.1",
        "laravel/breeze": "^1.6",
        "laravel/pint": "^0.2.0",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.2",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0",
        "thedevsaddam/laravel-schema": "^2.0",
        "wulfheart/laravel-actions-ide-helper": "^0.3.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan filament:upgrade"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "test": "vendor/bin/pest",
        "model-docs": "php artisan ide-helper:models"
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "barryvdh/laravel-ide-helper"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform-check": false,
        "platform": {
            "php": "8.1"
        },
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "php-http/discovery": true
        }
    },
    "platform": {
        "php": "8.1"
    },
    "platform-check": false,
    "minimum-stability": "dev",
    "prefer-stable": true
}
